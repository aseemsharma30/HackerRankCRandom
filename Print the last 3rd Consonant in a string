#include <stdio.h>
int main ()
{
  char n[100], a[100];
  int z, x = 0;
  
  scanf ("%s", &n);
  z = strlen (n);
  for (int i = 0; i < z; i++)
    {
      if (n[i] != 'a' && n[i] != 'e' && n[i] != 'i' && n[i] != 'o' && n[i] != 'u')
	    {
	        x = x + 1;
	        a[x] = n[i];
	    }
    }
  printf ("%c", a[x - 2]);
  return 0;
}
